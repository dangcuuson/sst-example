{
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
      "tsc": "tsc -p ./tsconfig.json",
      "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
      "test": "playwright test -c ./playwright-ct/playwright-ct.config.ts",
      "test:e2e": "playwright test -c ./playwright/playwright.config.ts",
      "prebuild": "npm run build-env",
      "build": "vite build",
      "build-gql": "graphql-codegen -c ./scripts/graphql-codegen.ts",
      "build-env": "ts-node -TP ../../tsconfig.node.json ./scripts/buildEnv.ts",
      "dev": "vite --port 3000",
      "bundle-review": "vite-bundle-visualizer -o bundle-review.html --open"
  },
  "devDependencies": {
      "@parcel/watcher": "2.3.0",
      "@playwright/experimental-ct-react": "^1.40.1",
      "@playwright/test": "1.40.1",
      "@types/lodash": "4.14.202",
      "@types/node": "20.10.6",
      "@types/react": "18.2.45",
      "@types/react-dom": "18.2.18",
      "@vitejs/plugin-react-swc": "3.5.0",
      "git-branch": "2.0.1",
      "vite": "5.0.10",
      "vite-bundle-visualizer": "1.0.0"
  },
  "dependencies": {
      "@apollo/client": "3.8.8",
      "@aws-amplify/ui-react": "6.0.7",
      "@graphql-codegen/cli": "5.0.0",
      "@graphql-codegen/client-preset": "4.1.0",
      "apollo3-cache-persist": "0.14.1",
      "aws-amplify": "6.0.9",
      "graphql": "16.8.1",
      "graphql-ws": "5.14.3",
      "lodash": "4.17.21",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-icons": "4.12.0",
      "react-router": "6.21.0",
      "react-router-dom": "6.21.0",
      "styled-components": "6.1.3",
      "subscriptions-transport-ws": "0.11.0",
      "uuid": "9.0.1",
      "zod": "3.22.4",
      "zustand": "4.4.7"
  }
}
